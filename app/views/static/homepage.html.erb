<div class='body'>
  <div id='static_container'>
    <div class='static_img'>
      <%= image_tag('exercise-stock-edited.png') %>
    </div>
  </div>

  <div class='container-fluid'>
    <div class='horizontal-equipment '>
      <div class='equipment-container'>
        <div class='images'>
          <%= image_tag('Bike2.png') %>
          <p>Bikes </p>
        </div>
        <div class='images'>
          <%= image_tag('Elliptical2.png') %>
          <p>Ellipticals </p>
        </div>
        <div class='images'>
          <%= image_tag('sequip2.png') %>
          <p>Strength</p>
        </div>
        <div class='images'>
          <%= image_tag('Stepper2.png') %>
          <p>Steppers</p>
        </div>
        <div class='images'>
          <%= image_tag('Treadmill2.png') %>
          <p>Treadmills</p>
        </div>
      </div>
    </div>
  </div>
  <div class='divider'></div>
  <div id='vendor-images'>
    <div class='container-fluid'>
      <!-- <h2> OUR VENDORS </h2> -->
      <div class='vendor_row row'>
        <div class='vendor-col col-md'>
          <%= image_tag('CYBEX_Logo.png') %>
        </div>
        <div class='vendor-col col-md'>
          <%= image_tag('freemotion_logo.jpg') %>
        </div>
        <div class='vendor-col col'>
          <%= image_tag('LF_logo.png') %>
        </div>
      </div>
      <div class='vendor_row row'>
        <div class='vendor-col col-md'>
          <%= image_tag('Precor_logo.png') %>
        </div>
        <div class='vendor-col col-md'>
          <%= image_tag('stairmaster_logo.jpg') %>
        </div>
        <div class='vendor-col col-md'>
          <%= image_tag('star-trac-logo.png') %>
        </div>

      </div>
      <p> AND MORE!</p>
    </div>
  </div>
</div>
